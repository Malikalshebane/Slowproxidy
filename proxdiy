    --Taps
    local Tab = Window:MakeTab({
            Name = "AutoFarm",
            Icon = "rbxassetid://4483345998",
            PremiumOnly = false
        })
        local Tabs = Window:MakeTab({
            Name = "Combat",
            Icon = "rbxassetid://4483345998",
            PremiumOnly = false
        })


    --Buttons
    Tab:AddToggle({
        Name = "Auto Treadmill",
        Default = false,
        Callback = function(Value)
            getgenv().autofarmtread = Value
            autoautofarm()
        end    
    })
    Tab:AddToggle({
        Name = "Auto Bench",
        Default = false,
        Callback = function(Value)
            getgenv().autobenchpress = Value
            autobenchpressfull()
        end    
    })
    Tab:AddToggle({
        Name = "Auto Riatsu",
        Default = false,
        Callback = function(Value)
            getgenv().autoriatsu = Value
            autoriatsufarm()
        end    
    })
    Tab:AddToggle({
        Name = "Auto Spirit",
        Default = false,
        Callback = function(Value)
            getgenv().autoriatsu = Value
            autospiritfarm()
        end    
    })

    Tabs:AddToggle({
        Name = "Unnoticeable No Stun",
        Default = false,
        Callback = function(Value)
            getgenv().Nostun = Value
            nostuns()
        end    
    })

    Tabs:AddToggle({
        
        Name = "Unnoticeable Auto Parry",
        Default = false,
        Callback = function(Value)
            print(Value)
            getgenv().autoparrys = Value
            autopar()
        end    
    })
